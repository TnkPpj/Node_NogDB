require =>
    make :
        const nogdb = require('../addon/build/Debug/nogdb');

Context =>
    make :
        var ctx = new nogdb.Context("test.db");   // test.db : nameDB

    funtion :
        ctx.getMaxVersionId();
        ctx.getMaxTxnId();
        ctx.getMinActiveTxnId();

Txn =>
    make :
        var txn = new nogdb.Txn(ctx,"READ_WRITE");   // ctx : Context , "READ_WRITE" : mode READ_WRITE , READ_ONLY
    
    funtion :
        txn.commit();
        txn.rollback();
        txn.getTxnId();
        txn.getVersionId();
        txn.getTxnMode();

DBInfo =>
    make : 
        var dbInfo = new nogdb.DBInfo(txn);     // txn : Txn
    
    Attribute :
        dbInfo.dbPath;
        dbInfo.maxDB;
        dbInfo.maxDBSize;
        dbInfo.maxPropertyId;
        dbInfo.numProperty;
        dbInfo.maxClassId;
        dbInfo.numClass;

    funtion :
        dbInfo.getDbInfo(txn);

ClassDescriptor =>
    make :
        var classDesc = new nogdb.ClassDescriptor(txn);     // txn : Txn
    
    Attribute :
        classDesc.id;
        classDesc.name;
        classDesc.type;
        classDesc.properties;
        classDesc.super;
        classDesc.sub;

